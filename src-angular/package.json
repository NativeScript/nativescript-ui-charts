{
  "name": "@nativescript/ui-charts-angular",
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@nativescript/core": "ns-core",
        "@nativescript/angular": "ns-angular",
        "@nativescript/ui-charts": "ns-ui-charts"
      }
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
